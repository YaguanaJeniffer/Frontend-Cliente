{
    "source": {
      "include": [
        "src/"
      ],
      "exclude": [
        "node_modules/"
      ]
    },
    "opts": {
      "template": "./node_modules/jsdoc-template",
      "encoding": "utf8",
      "destination": "./docs"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "default": {
        "outputSourceFiles": true
      }
    }
  }
  